              <modifiers/>
              <rules>
                <rule id="4f9b-1976-f219-2352" name="Healing Mist" hidden="false">
                  <description>The madoushi may use its action
to Stun itself, then remove all Stun tokens from
up to two friendly models within four spaces of
the madoushi. No LoS is required.</description>
                  <modifiers/>
                </rule>
              </rules>
              <profiles/>
              <links/>
            </entry>
            <entry id="47d6-6727-b849-45ba" name="Wander Lust" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false">
